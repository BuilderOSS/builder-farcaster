{
  "name": "builder-farcaster",
  "version": "1.0.5",
  "description": "Tools and automation solutions for Builder DAO on Farcaster, enhancing community efficiency and engagement.",
  "type": "module",
  "scripts": {
    "prebuild": "pnpm prisma:migrate && pnpm prisma:generate",
    "build": "vite build",
    "predev": "pnpm lint && pnpm build",
    "dev": "pnpm dev:process && pnpm dev:consume",
    "dev:consume": "node dist/index.js queues consume -l 10",
    "dev:invites": "node dist/index.js process invites",
    "dev:process": "node dist/index.js process proposals",
    "dev:tokengen": "node dist/index.js warpcast token",
    "lint": "eslint",
    "prepare": "husky || :",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "pretest": "pnpm build",
    "test": "vitest run --passWithNoTests"
  },
  "resolutions": {
    "elliptic": ">=6.6.0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "commander": "12.1.0",
    "crypto-js": "4.2.0",
    "dotenv": "16.4.5",
    "graphql": "16.9.0",
    "graphql-request": "7.1.2",
    "luxon": "3.5.0",
    "pino": "9.5.0",
    "pino-pretty": "12.1.0",
    "prisma": "5.22.0",
    "remeda": "2.17.0",
    "type-fest": "4.26.1",
    "uuid": "11.0.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@types/crypto-js": "4.2.2",
    "@types/eslint__js": "8.42.3",
    "@types/luxon": "3.4.2",
    "@types/node": "22.9.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/parser": "8.13.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "50.4.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-prisma": "5.0.0",
    "prettier-plugin-toml": "2.0.1",
    "typescript-eslint": "8.13.0",
    "vite": "5.4.10",
    "vite-plugin-node-polyfills": "0.22.0",
    "vite-tsconfig-paths": "5.1.0",
    "vitest": "2.1.4"
  }
}
